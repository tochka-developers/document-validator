{
    "name": "tochka-developers/document-validator",
    "description": "Laravel validation for documents codes",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Barakovskikh",
            "email": "hatsune333@gmail.com"
        }
    ],
    "require": {
        "php": "7.4.*|8.0.*|8.1.*",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.1",
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "<9.5.17"
    },
    "autoload": {
        "psr-4": {
            "Tochka\\Validators\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tochka\\Validators\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tochka\\Validators\\DocumentValidatorServiceProvider"
            ]
        }
    }
}
